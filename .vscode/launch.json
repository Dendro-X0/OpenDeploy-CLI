{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run OpenDeploy Extension (apps/extension)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/apps/extension"
      ],
      "outFiles": ["${workspaceFolder}/apps/extension/dist/**/*.js"],
      "preLaunchTask": "ext:build"
    }
  ]
}
